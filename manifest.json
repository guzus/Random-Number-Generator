{
    "name": "Random Number Generator",
    "version": "1.1",
    "description": "Simple random number generator for productivity",
    "permissions": [
        "storage"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "images/random.001.png"
        }
    },
    "icons": {
        "128": "images/random.001.png"
    },
    "manifest_version": 2
}